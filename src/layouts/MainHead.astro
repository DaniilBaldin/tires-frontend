---
const { title } = Astro.props;
---

<head>
    <meta charset='UTF-8' />
    <meta
        name='description'
        content='Tires site.'
    />
    <meta
        name='viewport'
        content='width=device-width'
    />
    <link
        rel='icon'
        type='image/png'
        href='/TireLogo.png'
    />
    <meta
        name='generator'
        content={Astro.generator}
    />
    <title>{title}</title>
</head>
